{
  "name": "opendota-ui",
  "description": "Dota 2 Data Web UI",
  "version": "1.0.1",
  "license": "MIT",
  "scripts": {
    "start": "NODE_PATH=src react-scripts start",
    "build": "npm install && npm run lintnofix && npm run rsbuild",
    "rsbuild": "NODE_PATH=src react-scripts build",
    "test": "NODE_PATH=src react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "webpack": "NODE_ENV=production webpack",
    "lint": "eslint . --ext .js,.jsx --fix",
    "lintnofix": "eslint . --ext .js,.jsx",
    "update:emoticons": "npm r dota2-emoticons && npm i dota2-emoticons && node dev/updateEmoticons.js"
  },
  "greenkeeper": {
    "label": "type: greenkeeper"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/odota/ui"
  },
  "dependencies": {
    "abcolor": "^0.5.5",
    "dota2-emoticons": "^1.0.1",
    "dotaconstants": "^5.11.0",
    "fuzzy": "^0.1.3",
    "heatmap.js": "^2.0.5",
    "isomorphic-fetch": "^2.2.1",
    "json2csv": "^3.10.0",
    "lodash": "^4.17.4",
    "long": "^3.2.0",
    "material-ui": "^0.19.0",
    "prop-types": "^15.5.10",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-helmet": "^5.1.3",
    "react-markdown": "^2.5.0",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.2",
    "react-scripts": "1.0.14",
    "react-style-proptype": "^3.0.0",
    "react-tooltip": "^3.3.0",
    "recharts": "^1.0.0-apha.5",
    "redux": "^3.7.2",
    "redux-responsive": "^4.3.2",
    "redux-thunk": "^2.2.0",
    "seamless-immutable": "^7.1.2",
    "styled-components": "^2.2.0",
    "uuid": "^3.1.0",
    "wordcloud": "^1.0.6"
  },
  "devDependencies": {
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.2.0",
    "fs-extra": "^4.0.1",
    "simple-vdf": "^1.1.0"
  }
}
